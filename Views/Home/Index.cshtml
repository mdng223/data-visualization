@{
    ViewData["Title"] = "Home Page";
}

<div id="app">
  <h1>Hello App!</h1>
  <p>
    <!-- use router-link component for navigation. -->
    <!-- specify the link by passing the `to` prop. -->
    <!-- `<router-link>` will be rendered as an `<a>` tag by default -->
    <router-link to="/foo">Go to Foo</router-link>
    <router-link to="/bar">Go to Bar</router-link>
    <router-link to="/user">Go to User</router-link>
  </p>
  <!-- route outlet -->
  <!-- component matched by the route will render here -->
  <router-view ></router-view>
  
    <template>
        <div class="loading" v-if="loading">
        Loading...
        </div>
    </template>


</div>
@section Scripts {
    <script type="module" src="~/js/pages/userPage.js"></script>
    <script type="module" src="~/js/components/debugButton.js"></script>
    <script type="text/javascript" src="~/js/user/validate.js"></script>
    <script type="text/javascript" src="~/js/services/userRequests.js"></script>
    <script type="module" src="~/js/app.js"></script>
    
}